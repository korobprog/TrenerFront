# Включение реальных ссылок на Google Meet в режиме разработки
ENABLE_REAL_MEET_LINKS=true

# Настройки базы данных
DATABASE_URL=postgresql://user:password@localhost:5432/interview_prep

# Базовые настройки Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Настройки Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Настройки для Google Calendar API
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google
# Следующие токены должны храниться в базе данных, а не в .env файлах
# GOOGLE_ACCESS_TOKEN=
# GOOGLE_REFRESH_TOKEN=
# GOOGLE_TOKEN_EXPIRY=